<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <!-- Billetweb Payment Provider -->
        <record id="payment_provider_billetweb" model="payment.provider">
            <field name="name">Billetweb</field>
            <field name="code">billetweb</field>
            <field name="state">disabled</field>
            <field name="is_published">False</field>
            <field name="sequence">50</field>
            <field name="image_128" type="base64" file="payment_billetweb/static/description/icon.png"/>
            <field name="redirect_form_view_id" ref="payment_billetweb.redirect_form"/>
            <field name="support_refund">full_only</field>
            <field name="support_tokenization">False</field>
            <field name="support_manual_capture">False</field>
        </record>

    </data>
</odoo>