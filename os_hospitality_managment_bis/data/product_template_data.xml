    <!-- Produit Commission Booking -->
    <record id="product_booking_commission" model="product.template">
        <field name="name">Commission Booking.com</field>
        <field name="default_code">COMMISSION_BOOKING</field>
        <field name="type">service</field>
        <field name="categ_id" ref="product_category_commissions"/>
        <field name="list_price">0.0</field>
        <field name="standard_price">0.0</field>
        <field name="uom_id" ref="uom.product_uom_unit"/>
        <field name="uom_po_id" ref="uom.product_uom_unit"/>
        <field name="purchase_ok">True</field>
        <field name="sale_ok">False</field>
        <field name="can_be_expensed">True</field>
        <field name="property_account_expense_id" ref="account_commissions"/>
        <field name="description">Commission versée à Booking.com - Montant variable selon réservation</field>
    </record>

