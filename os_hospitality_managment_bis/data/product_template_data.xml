<odoo noupdate="1">
    <!-- Produit : Taxe de séjour pour Meublé de tourisme -->
    <record id="product_template_tourist_tax_3" model="product.template">
        <field name="sequence">3</field>
        <field name="standard_price">0.0</field>
        <field name="name">Meublé de tourisme</field>
        <field name="list_price">0.0</field>
        <field name="default_code">TDS</field>
        <field name="categ_id" ref="product_category_tourist_tax"/>
        <field name="uom_id" ref="uom.product_uom_unit"/>
        <field name="uom_po_id" ref="uom.product_uom_unit"/>
        <field name="type">service</field>
        <field name="taxes_id" eval="[(6, 0, [ref('account_tax_tourist_tax_3')])]"/>
        <field name="purchase_ok">True</field>
        <field name="property_account_expense_id" ref="account_tourist_tax"/>
        <field name="sale_ok">True</field>
    </record>

