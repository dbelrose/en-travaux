<?xml version="1.0"?>
<odoo noupdate="1">

    <record id="res_users_pf_dsi_user" model="res.users">
        <field name="server_mail_id" ref="ir_mail_server_pf_gov_dsi"/>
        <field name="name">Utilisateur PF-GOV-SI</field>
        <field name="login">user@informatique.gov.pf</field>
        <field name="lang">fr_FR</field>
        <field name="tz">Pacific/Tahiti</field>
        <field name="company_id" ref="base.main_company"/>
        <field name="company_ids" eval="[(6, 0, [ref('base.main_company')])]"/>
        <field name="groups_id" eval="[(6, 0, [ref('res_groups_pf_gov_dsi_user')])]"/>
        <field name="is_company" eval="False"/>
        <field name="employee" eval="True"/>
        <field name="active" eval="False"/>
    </record>

    <record id="res_users_pf_dsi_manager" model="res.users">
        <field name="name">Utilisateur PF-GOV-SI</field>
        <field name="login">manager@informatique.gov.pf</field>
        <field name="lang">fr_FR</field>
        <field name="tz">Pacific/Tahiti</field>
        <field name="company_id" ref="base.main_company"/>
        <field name="company_ids" eval="[(6, 0, [ref('base.main_company')])]"/>
        <field name="groups_id" eval="[(6, 0, [ref('res_groups_pf_gov_dsi_manager')])]"/>
        <field name="is_company" eval="False"/>
        <field name="employee" eval="True"/>
        <field name="active" eval="False"/>
    </record>

</odoo>
