<odoo>
    <data>
        <record id="view_partner_form_billetweb" model="ir.ui.view">
            <field name="name">res.partner.form.billetweb</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="base.view_partner_form"/>
            <field name="arch" type="xml">
                <notebook position="inside">
                    <page string="Virements BilletWeb">
                        <field name="billetweb_payout_ids">
                            <tree>
                                <field name="payout_id"/>
                                <field name="date"/>
                                <field name="amount"/>
                                <field name="imported_payment_id"/>
                            </tree>
                        </field>
                    </page>
                    <page string="Factures de commission">
                        <field name="billetweb_invoice_ids">
                            <tree>
                                <field name="name"/>
                                <field name="invoice_date"/>
                                <field name="amount_total"/>
                                <field name="state"/>
                            </tree>
                        </field>
                    </page>
                </notebook>
            </field>
        </record>
    </data>
</odoo>