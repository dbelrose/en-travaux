<odoo>
    <data noupdate="True">
        <record id="facturer" model="ir.cron">
            <field name="name">Calculer les factures</field>
            <field name="user_id" ref="base.user_root" />
            <field name="numbercall">-1</field>
            <field name="model_id" ref="model_manureva_facture" />
            <field name="state" eval="'code'" />
            <field name="active" eval="True" />
            <field name="interval_number">1</field>
            <field name="interval_type">months</field>
            <field name="doall">1</field>
            <field name="nextcall" >2020-12-31 23:59:59</field>
            <field name="priority" eval="5" />
			<!-- <field name="activity_user_type" eval="'specific'"/> -->
            <field name="code" eval="'model.calculer_facture()'" />
        </record>
    </data>
</odoo>