<?xml version="1.0" encoding="utf-8"?>
<odoo>
        <record id="view_tva_tree" model="ir.ui.view">
            <field name="name">manureva.tva.tree</field>
            <field name="model">manureva.tva</field>
            <field name="arch" type="xml">
                <tree string="TVA">
                    <field name="name"/>
                    <field name="taux"/>
                    <field name="debut"/>
                    <field name="fin"/>
                </tree>
            </field>
        </record>

        <record id="action_tva" model="ir.actions.act_window">
            <field name="name">TVA</field>
            <field name="res_model">manureva.tva</field>
            <field name="view_mode">tree,form</field>
            <field name="domain">[]</field>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">Créez votre première TVA
                </p>
            </field>
        </record>

    	<menuitem 	id="menu_tva"
					parent="menu_taxe"
					name="TVA"
					action="action_tva"/>
</odoo>