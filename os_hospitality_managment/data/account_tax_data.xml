<?xml version="1.0" encoding="UTF-8"?>
<odoo noupdate="1">
    <!-- Taxe de séjour pour les Meublés de tourisme -->
    <record id="account_tax_tourist_tax_3" model="account.tax">
        <field name="active">True</field>
        <field name="description">Meublé de tourisme</field>
        <field name="invoice_label">Tourist tax</field>
        <field name="name">Meublé de tourisme</field>
        <field name="tax_scope">service</field>
        <field name="sequence">3</field>
        <field name="type_tax_use">sale</field>
        <field name="amount_type">fixed</field>
        <field name="tax_group_id" ref="account_tax_group_tourist_tax"/>
        <field name="amount">60.0</field>
        <field name="country_id" ref="base.pf"/>
    </record>

